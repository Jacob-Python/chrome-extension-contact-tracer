{
  "manifest_version": 2,
  "name": "CoronaNet v0.1",
  "version": "0.1",
  "description": "This app is designed to aid in staying safe during the pandemic.",
  "permissions":["storage","geolocation","tabs","https://contacttracerapp.herokuapp.com/*"],
  "browser_action":{
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
    "icons": {
    "16": "assets/favicon-16x16.png",
    "32": "assets/favicon-32x32.png"
  }
}